<div class="card">
  <div class="card-header">
    <h3>Categorias </h3>
    <a href="/admin/categorias/add"><button>Criar uma nova categoria</button></a>
  </div>
  <div class="card-body">
    {{#each categorias}}
      <a href="/categorias/{{slug}}"><h4 class="card-title ">{{nome}}</h3></a>
    {{else}}
    {{/each}}
  </div>
</div>
